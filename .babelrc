{
    "presets": [
        ["next/babel", {
            "preset-react": {
              "runtime": "automatic",
              "importSource": "@emotion/react"
            },
            "styled-jsx": {
              "plugins": ["@styled-jsx/plugin-sass"],
              "optimizeForSpeed": true,
            }
          }],
    ],
    "plugins": [
        [ "styled-components", { "ssr": true } ],
        "@emotion/babel-plugin",
    ]
}